{
	"name": "gitapi",
	"type": "module",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "dotenv -e .env -- concurrently \"next dev\" \"bun run db:server\"",
		"build": "next build",
		"start": "./scripts/start.sh",
		"lint": "next lint",
		"db:migrate": "bun run src/db/migrate.ts",
		"db:seed": "bun run src/db/seed.ts",
		"db:server": "dotenv -e .env -- bun run src/server.ts",
		"db:status": "bun --bun src/scripts/check-db.ts",
		"test": "bun test",
		"test:watch": "bun test --watch",
		"migrate": "bun run src/db/migrate.ts",
		"stop": "./scripts/stop.sh"
	},
	"devDependencies": {
		"@shadcn/ui": "^0.0.4",
		"@types/supertest": "^6.0.2",
		"concurrently": "^9.1.2",
		"dotenv-cli": "^8.0.0",
		"supertest": "^7.0.0",
		"typescript": "^5.7.2"
	},
	"peerDependencies": {
		"typescript": "^5.7.2"
	},
	"dependencies": {
		"@fontsource/geist-sans": "^5.1.0",
